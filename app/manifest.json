{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "dKeeper "
  },
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "offline_enabled": false,
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [ "tabs", "\u003Call_urls>", "http://*/*", "https://*/*", "notifications", "identity", "clipboardWrite", "clipboardRead", "contextMenus"]
}
